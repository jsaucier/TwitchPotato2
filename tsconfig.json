{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "./app",
        "sourceMap": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./source/app.ts",
        "./source/js/authenticator.ts",
        "./source/js/chat.ts",
        "./source/js/enums.ts",
        "./source/js/guide/content.ts",
        "./source/js/guide/contextmenu.ts",
        "./source/js/guide/guide.ts",
        "./source/js/guide/itemmenu.ts",
        "./source/js/guide/list.ts",
        "./source/js/guide/menu.ts",
        "./source/js/guide/menuitem.ts",
        "./source/js/guide/timer.ts",
        "./source/js/input.ts",
        "./source/js/lib/chrome.d.ts",
        "./source/js/lib/definitions.d.ts",
        "./source/js/lib/jquery.d.ts",
        "./source/js/notification.ts",
        "./source/js/player/controller.ts",
        "./source/js/player/menu.ts",
        "./source/js/player/player.ts",
        "./source/js/player/players.ts",
        "./source/js/potato.ts",
        "./source/js/settings.ts",
        "./source/js/timers.ts",
        "./source/js/twitch.ts",
        "./source/js/utils.ts",
        "./source/js/webview.ts",
        "./source/js/windows.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
